{
    "input_model": {
        "type": "HfModel",
        "model_path": "Qwen/Qwen2.5-1.5B-Instruct"
    },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "accelerators": [
                {
                    "device": "cpu",
                    "execution_providers": [
                        "CPUExecutionProvider"
                    ]
                }
            ]
        }
    },
    "data_configs": [
        {
            "name": "inc_static_quant_data_config",
            "user_script": "user_script.py",
            "load_dataset_config": {
                "type": "simple_dataset"
            },
            "dataloader_config": {
                "type": "kv_dataloader",
                "batch_size": 1,
                "n_batches": 16
            }
        }
    ],
    "evaluators": {
        "winog_acc_evaluator": {
            "type": "LMEvaluator",
            "tasks": [
                "winogrande"
            ],
            "batch_size": 1,
            "max_length": 128
        },
        "wt2_ppl_evaluator": {
            "metrics": [
                {
                    "name": "wt2-ppl",
                    "type": "custom",
                    "sub_types": [
                        {
                            "name": "custom"
                        }
                    ],
                    "user_config": {
                        "user_script": "user_script.py",
                        "evaluate_func": "eval_wt2_ppl",
                        "evaluate_func_kwargs": {
                            "tasks": [
                                "wikitext"
                            ],
                            "batch_size": 128
                        }
                    }
                }
            ]
        }
    },
    "passes": {
        "convert": {
            "type": "ModelBuilder",
            "precision": "fp32",
            "override_search_opt": false
        },
        "quantization": {
            "type": "IncStaticQuantization",
            "quant_format": "QOperator",
            "user_script": "user_script.py",
            "recipes": {
                "smooth_quant": true,
                "smooth_quant_args": {
                    "alpha": 0.7
                },
                "graph_optimization_level": "ENABLE_EXTENDED",
                "optypes_to_exclude_output_quant": [
                    "MatMul"
                ]
            },
            "tuning_criterion": {
                "strategy": "basic",
                "strategy_kwargs": null,
                "timeout": 0,
                "max_trials": 5,
                "objective": "accuracy"
            },
            "op_type_dict": {
                "^((?!(MatMul|Gather|Conv)).)*$": {
                    "weight": {
                        "dtype": [
                            "fp32"
                        ]
                    },
                    "activation": {
                        "dtype": [
                            "fp32"
                        ]
                    }
                }
            },
            "excluded_precisions": [
                "bf16",
                "fp16"
            ],
            "data_config": "inc_static_quant_data_config",
            "calibration_sampling_size": [
                100
            ],
            "save_as_external_data": true,
            "all_tensors_to_one_file": true
        }
    },
    "evaluate_input_model": false,
    "evaluator": "wt2_ppl_evaluator",
    "host": "local_system",
    "target": "local_system",
    "cache_dir": "cache",
    "output_dir": "models/qwen2_smooth"
}
